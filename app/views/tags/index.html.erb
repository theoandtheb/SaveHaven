<h1>This is the tags index</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <!-- <th>Note</th> -->
      <!-- <th>Url</th> -->
      <!-- <th>Audio</th> -->
      <!-- <th>Video</th> -->
      <!-- <th>Image</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= tag.location %></td>
        <!-- <td><%= tag.note %></td> -->
        <!-- <td><%= tag.url %></td> -->
        <!-- <td><%= tag.audio %></td> -->
        <!-- <td><%= tag.video %></td> -->
        <!-- <td><%= tag.image %></td> -->
        <td><%= link_to 'Show', tag %></td>
        <td><%= link_to 'Edit', edit_tag_path(tag) %></td>
        <td><%= link_to 'Destroy', tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag', new_tag_path %>
